{
  "name": "Github Tracker",
  "version": "1.0.0",
  "manifest_version": 3,
  "author": "Aaradhya Saxena & Anirudh Dagar",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Github Tracker"
  }
}

